@model IEnumerable<mv.DataModel.Users>
<br />
<br />
@foreach (var item in Model)
{
    <div class="col-md-3">
        <a href="@Url.Action("About", "ProfilePage",new { uid = item.ID })">
            <img src="@Url.Content(@item.PictureLoc)" class="img-circle" style="max-height:100px;max-width:100px" />
        </a>
        <h3>@item.Name @item.Surname</h3>
    </div>
}